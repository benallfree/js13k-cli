    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "tsdown": "^0.14.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "express": "^5.1.0",
    "hono": "^4.9.2",
    "js13k-vite-plugins": "^0.4.0",
    "partykit": "^0.0.115",
    "vite": "^7.1.2",
    "ws": "^8.18.3"
  },
  "trustedDependencies": [
    "advzip-bin",
    "ect-bin"
  ],
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
